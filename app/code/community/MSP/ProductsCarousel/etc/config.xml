<config>
	<modules>
		<MSP_ProductsCarousel>
			<version>1.1.0</version>
		</MSP_ProductsCarousel>
	</modules>
	
	<global>
		<models>
			<msp_productscarousel>
				<class>MSP_ProductsCarousel_Model</class>
			</msp_productscarousel>
        </models>
		
		<helpers>
			<msp_productscarousel>
				<class>MSP_ProductsCarousel_Helper</class>
			</msp_productscarousel>
        </helpers>

		<blocks>
			<msp_productscarousel>
				<class>MSP_ProductsCarousel_Block</class>
			</msp_productscarousel>
        </blocks>
	</global>
	
	<frontend>
		<layout>
			<updates>
				<msp_productscarousel module="MSP_ProductsCarousel">
					<file>msp_productscarousel.xml</file>
				</msp_productscarousel>
			</updates>
		</layout>
		
		<translate>
			<modules>
				<MSP_ProductsCarousel>
					<files>
						<default>MSP_ProductsCarousel.csv</default>
					</files>
				</MSP_ProductsCarousel>
			</modules>
		</translate>
	</frontend>
</config>
